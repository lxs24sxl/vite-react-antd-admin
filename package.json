{
  "name": "integral-admin",
  "version": "1.0.0",
  "private": true,
  "author": "lxs24sxl",
  "scripts": {
    "start": "vite --mode dev",
    "start:prod": "vite --mode prod",
    "build": "tsc && vite build --mode build-dev",
    "serve": "vite preview",
    "prebuild:dev": "tsc && vite build --mode build-dev",
    "prebuild:prod": "tsc && vite build --mode build-prod",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint": "npm run lint:js",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx ./src && npm run lint:style"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts}": [
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "^4.16.13",
    "autoprefixer": "^10.2.5",
    "axios": "^0.21.1",
    "classnames": "^2.3.0",
    "history": "^4.10.1",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "postcss": "^8.2.8",
    "qs": "^6.10.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.36",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.13",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.1",
    "@types/redux-logger": "^3.0.8",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "4.0.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.5.4",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "redux-logger": "^3.0.6",
    "rollup-plugin-visualizer": "^5.5.2",
    "stylelint": "^13.12.0",
    "stylus": "^0.54.8",
    "typescript": "^4.1.2",
    "vite": "^2.5.0",
    "vite-plugin-html-env": "^1.0.1-4",
    "vite-plugin-style-import": "^0.9.1"
